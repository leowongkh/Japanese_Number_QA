<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" prefHeight="300.0" prefWidth="225.0" spacing="20.0"
      xmlns="http://javafx.com/javafx/11.0.1" fx:controller="hk.edu.nihongokoza.controller.MainMenuController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
   </padding>
   <Label alignment="CENTER" textAlignment="CENTER" fx:id="titleLabel">
      <font>
         <Font name="System Bold" size="16.0"/>
      </font>
   </Label>
   <Button onAction="#startGame" fx:id="startButton"/>
   <Button onAction="#switchToAnswerHistory" fx:id="answerHistoryButton"/>
   <Button mnemonicParsing="false" onAction="#leaveApp" fx:id="exitButton"/>
</VBox>
